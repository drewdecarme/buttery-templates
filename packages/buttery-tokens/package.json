{
  "name": "@buttery/tokens",
  "version": "1.0.0",
  "description": "A CLI to create a buttery collection of utilities for various CSS strategies to easily and safely work with your tokens",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/drewdecarme/buttery-tokens.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "buttery-tokens": "./bin/index.js"
  },
  "type": "module",
  "author": "Drew DeCarme (drewdecarme@gmail.com)",
  "license": "Apache-2.0",
  "homepage": "https://github.com/drewdecarme/buttery-tokens#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/drewdecarme/buttery-tokens/issues"
  },
  "scripts": {
    "build:tokens": "yarn buttery-tokens build",
    "build": "yarn buttery cli build && yarn build:tokens",
    "dev": "yarn buttery cli dev",
    "icons:build": "yarn icons:generate && yarn icons:types",
    "icons:generate": "svgr --out-dir ./playground/assets/icons/react --filename-case kebab --typescript --jsx-runtime automatic --no-index --title-prop -- ./playground/assets/icons/svg",
    "icons:types": "npx tsx ./playground/scripts/script.icon-types.ts",
    "docs:dev": "yarn buttery-docs dev",
    "docs:build": "yarn buttery-docs build"
  },
  "dependencies": {
    "@babel/core": "^7.24.6",
    "@babel/plugin-transform-export-namespace-from": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@buttery/cli": "workspace:^",
    "@buttery/core": "workspace:^",
    "@buttery/docs": "workspace:^",
    "@buttery/logger": "workspace:^",
    "@buttery/utils": "workspace:^",
    "@inquirer/prompts": "^5.0.3",
    "@linaria/core": "^6.2.0",
    "@linaria/react": "^6.2.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@wyw-in-js/babel-preset": "^0.5.1",
    "@wyw-in-js/vite": "^0.5.1",
    "chroma-js": "^2.4.2",
    "cosmiconfig": "^9.0.0",
    "esbuild": "^0.21.3",
    "glob": "^10.3.15",
    "immer": "^10.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "ts-pattern": "^5.1.2",
    "vite": "^5.2.12",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@buttery/tsconfig": "workspace:^",
    "@svgr/cli": "^8.1.0",
    "@svgr/core": "^8.1.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/chroma-js": "^2",
    "tsx": "^4.10.3",
    "typescript": "^5.4.5"
  },
  "buttery-tokens": {}
}
