{
  "name": "website",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "version": "1.0.0",
  "description": "the website",
  "scripts": {
    "build": "yarn tokens:build && yarn app:build",
    "app:build": "remix vite:build",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "tokens:build": "yarn buttery tokens build",
    "tokens:dev": "yarn buttery tokens dev"
  },
  "dependencies": {
    "@buttery/components": "workspace:",
    "@buttery/core": "workspace:",
    "@remix-run/cloudflare": "2.11.2",
    "@remix-run/cloudflare-pages": "2.11.2",
    "@remix-run/react": "2.11.2",
    "isbot": "5.1.17",
    "miniflare": "3.20240821.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@buttery/tokens": "workspace:",
    "@cloudflare/workers-types": "4.20240821.1",
    "@remix-run/dev": "2.11.2",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@typescript-eslint/parser": "8.3.0",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "node-fetch": "3.3.2",
    "typescript": "5.5.4",
    "vite": "5.4.2",
    "vite-tsconfig-paths": "5.0.1",
    "wrangler": "3.72.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
