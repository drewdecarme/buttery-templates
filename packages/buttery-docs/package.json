{
  "name": "@buttery/docs",
  "version": "0.1.4",
  "description": "TODO",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn tsx ./scripts/build-icons-and-tokens.ts && yarn typechecks && yarn lib:build && yarn cli-scripts:build",
    "typechecks": "tsc --project tsconfig.typechecks.json",
    "cli-scripts:build": "tsx ./scripts/cli-scripts.build.ts && yarn cli-scripts:types",
    "cli-scripts:types": "tsc --project tsconfig.cli-types.json",
    "lib:build": "tsx ./scripts/lib.build.ts && yarn lib:types",
    "lib:types": "tsc --project tsconfig.lib-types.json",
    "lib:dev": "yarn lib:build --watch",
    "local:dev": "tsx ./scripts/local.dev.ts",
    "local:build": "tsx ./scripts/local.build.ts",
    "tokens:dev": "tsx ./scripts/tokens.dev.ts",
    "tokens:build": "tsx ./scripts/tokens.build.ts",
    "icons:build": "tsx ./scripts/icons.build.ts",
    "pages:deploy": "cd ./.buttery/docs/dist && wrangler pages deploy ./build/client --project-name buttery-docs-cf",
    "pages:dev": "cd ./.buttery/docs/dist && wrangler pages dev ./build/client",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/drewdecarme/buttery-tools.git",
    "directory": "packages/buttery-docs"
  },
  "files": [
    "dist",
    "app"
  ],
  "author": "Drew DeCarme (drewdecarme@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drewdecarme/buttery-tools/issues"
  },
  "homepage": "https://github.com/drewdecarme/buttery-tools#readme",
  "exports": {
    "./cli/*": "./dist/cli-scripts/*.js",
    "./server": "./dist/server/index.js",
    "./client": "./dist/client/index.js",
    "./components": "./dist/components/index.js",
    "./css": "./dist/style.css"
  },
  "devDependencies": {
    "@buttery/icons": "workspace:*",
    "@buttery/tokens": "workspace:*",
    "@cloudflare/workers-types": "4.20241022.0",
    "@storybook/react": "8.3.6",
    "@types/babel__core": "7.20.5",
    "@types/express": "5.0.0",
    "@types/node": "22.8.2",
    "@types/react-dom": "18.3.1",
    "@wyw-in-js/vite": "0.5.4",
    "esbuild": "0.24.0",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "vite": "5.4.10"
  },
  "dependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-react": "7.25.9",
    "@babel/preset-typescript": "7.26.0",
    "@buttery/components": "workspace:^0.1.3",
    "@buttery/core": "workspace:^0.1.3",
    "@buttery/meta": "workspace:^0.1.3",
    "@linaria/core": "6.2.0",
    "@linaria/react": "6.2.1",
    "@linaria/server": "6.2.0",
    "@mdx-js/rollup": "3.1.0",
    "@shikijs/rehype": "1.22.2",
    "@stefanprobst/rehype-extract-toc": "2.2.0",
