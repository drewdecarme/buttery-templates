  "version": "0.0.1",
  "description": "Common node utilties that are used throughout all packages",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "yarn build --watch",
    "build": "yarn tsc --project tsconfig.build.json"
