{
  "name": "@buttery/cli",
  "version": "0.0.1",
  "description": "A file based CLI designed to making writing a CLI as easy as exporting a few funcitons in a file",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "dist",
    "templates"
  ],
  "type": "module",
  "scripts": {
    "build": "yarn tokens:build && yarn cli:build && yarn lib:build",
    "dev": "yarn cli:dev",
    "cli:dev": "tsx ./scripts/local.commands.dev",
    "cli:build": "tsx ./scripts/local.commands.build.ts",
    "tokens:build": "tsx ./scripts/local.tokens.build.ts",
    "tokens:dev": "tsx ./scripts/local.tokens.dev.ts",
    "lib:build": "yarn tsc --project tsconfig.lib.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "node": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./invoke": {
      "types": "./dist/invoke.d.ts",
      "import": "./dist/invoke.js",
      "node": "./dist/invoke.js",
      "require": "./dist/invoke.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/drewdecarme/starter-ts-cli.git"
  },
  "author": "Drew DeCarme (drewdecarme@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drewdecarme/starter-ts-cli/issues"
  },
  "homepage": "https://github.com/drewdecarme/starter-ts-cli#readme",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-react": "7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@buttery/core": "workspace:",
    "@buttery/tsconfig": "workspace:",
    "@chromatic-com/storybook": "1.7.0",
    "@cloudflare/workers-types": "4.20240821.1",
    "@ngneat/falso": "7.2.0",
    "@storybook/addon-essentials": "8.2.9",
    "@storybook/addon-interactions": "8.2.9",
    "@storybook/addon-links": "8.2.9",
    "@storybook/addon-onboarding": "8.2.9",
    "@storybook/blocks": "8.2.9",
    "@storybook/react": "8.2.9",
    "@storybook/react-vite": "8.2.9",
    "@storybook/test": "8.2.9",
    "@types/babel__core": "^7.20.5",
    "@types/body-parser": "1.19.5",
    "@types/express": "4.17.21",
    "@types/fs-extra": "11.0.4",
    "@types/node": "22.5.0",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0",
    "esbuild": "0.23.1",
    "storybook": "8.2.9",
    "tsx": "4.19.0",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@buttery/client": "workspace:",
    "@buttery/components": "workspace:",
    "@buttery/icons": "workspace:",
    "@buttery/logger": "workspace:",
    "@buttery/utils": "workspace:",
    "@inquirer/prompts": "5.3.8",
    "@linaria/core": "6.2.0",
    "@mdx-js/react": "3.0.1",
    "@mdx-js/rollup": "3.0.1",
    "@remix-run/cloudflare": "2.11.2",
    "@remix-run/dev": "^2.11.2",
    "@remix-run/express": "^2.11.2",
    "@remix-run/react": "2.11.2",
    "@shikijs/rehype": "1.14.1",
    "@types/mdx": "2.0.13",
    "@vitejs/plugin-react": "4.3.1",
    "@wyw-in-js/babel-preset": "^0.5.4",
    "@wyw-in-js/vite": "^0.5.4",
    "body-parser": "1.20.2",
    "chokidar": "3.6.0",
    "clsx": "2.1.1",
    "commander": "12.1.0",
    "cosmiconfig": "9.0.0",
    "express": "^4.19.2",
    "fs-extra": "11.2.0",
    "gray-matter": "4.0.3",
    "handlebars": "4.7.8",
    "install": "^0.13.0",
    "npm": "^10.8.2",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "rehype-autolink-headings": "7.1.0",
    "rehype-parse": "9.0.0",
    "rehype-shiki": "0.0.9",
    "rehype-slug": "6.0.0",
    "rehype-stringify": "10.0.0",
    "remark-frontmatter": "5.0.0",
    "remark-gfm": "4.0.0",
    "remark-mdx": "3.0.1",
    "remark-mdx-frontmatter": "^5.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.0",
    "rollup": "4.21.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-pattern": "5.3.1",
    "unified": "11.0.5",
    "unist-util-visit": "5.0.0",
    "vite": "5.4.2"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "buttery": "./bin/index.js"
  }
}
