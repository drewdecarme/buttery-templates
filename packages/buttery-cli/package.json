{
  "name": "@buttery/cli",
  "version": "0.0.1",
  "description": "A file based CLI designed to making writing a CLI as easy as exporting a few funcitons in a file",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "dist",
    "templates"
  ],
  "type": "module",
  "scripts": {
    "build": "yarn cli:build & yarn lib:build",
    "cli:dev": "yarn build --watch --local",
    "cli:build": "tsx ./scripts/build-local.ts",
    "lib:build": "yarn tsc --project tsconfig.lib.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "node": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./invoke": {
      "default": "./dist/invoke.js",
      "import": "./dist/invoke.js",
      "node": "./dist/invoke.js",
      "require": "./dist/invoke.js",
      "types": "./dist/invoke.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/drewdecarme/starter-ts-cli.git"
  },
  "author": "Drew DeCarme (drewdecarme@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drewdecarme/starter-ts-cli/issues"
  },
  "homepage": "https://github.com/drewdecarme/starter-ts-cli#readme",
  "devDependencies": {
    "@buttery/core": "workspace:^",
    "@buttery/tsconfig": "workspace:^",
    "@chromatic-com/storybook": "^1.5.0",
    "@remix-run/cloudflare": "^2.9.2",
    "@storybook/addon-essentials": "8.1.10",
    "@storybook/addon-interactions": "8.1.10",
    "@storybook/addon-links": "8.1.10",
    "@storybook/addon-onboarding": "8.1.10",
    "@storybook/blocks": "8.1.10",
    "@storybook/react": "8.1.10",
    "@storybook/react-vite": "8.1.10",
    "@storybook/test": "8.1.10",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.21.5",
    "storybook": "8.1.10",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@buttery/logger": "workspace:^",
    "@buttery/utils": "workspace:^",
    "@inquirer/prompts": "^5.0.6",
    "@linaria/core": "^6.2.0",
    "@linaria/react": "^6.2.1",
    "@mdx-js/react": "^3.0.1",
    "@mdx-js/rollup": "^3.0.1",
    "@remix-run/react": "^2.9.2",
    "@shikijs/rehype": "^1.9.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@wyw-in-js/babel-preset": "^0.5.3",
    "@wyw-in-js/vite": "^0.5.3",
    "chokidar": "^3.6.0",
    "clsx": "^2.1.1",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-parse": "^9.0.0",
    "rehype-shiki": "^0.0.9",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx": "^3.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "rollup": "^4.18.0",
    "ts-pattern": "^5.2.0",
    "unified": "^11.0.5",
    "vite": "^5.3.1"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "buttery": "./bin/index.js"
  }
}
