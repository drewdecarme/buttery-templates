{
  "name": "@buttery/cli",
  "version": "0.0.1",
  "description": "A file based CLI designed to making writing a CLI as easy as exporting a few funcitons in a file",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "dist",
    "templates"
  ],
  "type": "module",
  "scripts": {
    "build": "tsx ./scripts/build-local.ts",
    "dev": "yarn build --watch --local",
    "commands:build": "yarn butter cli build",
    "commands:dev": "yarn build --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/drewdecarme/starter-ts-cli.git"
  },
  "author": "Drew DeCarme (drewdecarme@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drewdecarme/starter-ts-cli/issues"
  },
  "homepage": "https://github.com/drewdecarme/starter-ts-cli#readme",
  "devDependencies": {
    "@buttery/core": "workspace:^",
    "@buttery/tsconfig": "workspace:^",
    "@chromatic-com/storybook": "^1.5.0",
    "@storybook/addon-essentials": "8.1.6",
    "@storybook/addon-interactions": "8.1.6",
    "@storybook/addon-links": "8.1.6",
    "@storybook/addon-onboarding": "8.1.6",
    "@storybook/blocks": "8.1.6",
    "@storybook/react": "8.1.6",
    "@storybook/react-vite": "8.1.6",
    "@storybook/test": "8.1.6",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.21.5",
    "storybook": "8.1.6",
    "tsx": "^4.15.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@buttery/logger": "workspace:^",
    "@buttery/utils": "workspace:^",
    "@inquirer/prompts": "^5.0.5",
    "@linaria/core": "^6.2.0",
    "@linaria/react": "^6.2.1",
    "@mdx-js/rollup": "^3.0.1",
    "@shikijs/rehype": "^1.7.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@wyw-in-js/babel-preset": "^0.5.3",
    "@wyw-in-js/vite": "^0.5.3",
    "chokidar": "^3.6.0",
    "clsx": "^2.1.1",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-parse": "^9.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx": "^3.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "rollup": "^4.18.0",
    "ts-pattern": "^5.2.0",
    "unified": "^11.0.4",
    "vite": "^5.3.1"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "buttery": "./bin/index.js"
  }
