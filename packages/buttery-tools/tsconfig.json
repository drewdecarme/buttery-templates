{
  "extends": "@buttery/tsconfig/vite/react",
  "compilerOptions": {
    "noEmit": true,
    "baseUrl": ".",
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "paths": {
      "@buttery/tokens/playground/css": [
        "./lib/buttery-tokens/.buttery-tokens/playground/index.css"
      ],
      // configuration ui tokens
      "@buttery/tokens/playground": [
        "./lib/buttery-tokens/.buttery-tokens/playground/index"
      ],
      "@buttery/tokens/docs/css": [
        "./lib/buttery-tokens/.buttery-tokens/docs/index.css"
      ],
      // configuration ui tokens
      "@buttery/tokens/docs": [
        "./lib/buttery-tokens/.buttery-tokens/docs/index"
      ]
    }
  }
}
