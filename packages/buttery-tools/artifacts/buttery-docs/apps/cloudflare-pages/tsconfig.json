{
  "include": [
    "./**/*.ts",
    "./**/*.tsx",
    "./**/.server/**/*.ts",
    "./**/.server/**/*.tsx",
    "./**/.client/**/*.ts",
    "./**/.client/**/*.tsx"
  ],
  "compilerOptions": {
    "lib": ["DOM", "ESNext"],
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "esModuleInterop": true,
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["@cloudflare/workers-types"],
    "baseUrl": ".",
    "paths": {
      "__ROUTE_MANIFEST__": [
        "../../../../.buttery/.store/docs/route-manifest.ts"
      ]
    }
  }
}
