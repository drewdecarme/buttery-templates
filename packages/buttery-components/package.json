{
  "name": "@buttery/components",
  "version": "0.0.0",
  "description": "A collection of components designed to make creating interfaces buttery smooth",
  "author": "Drew DeCarme (drewdecarme@gmail.com)",
  "type": "module",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./css": {
      "types": "./dist/index.d.ts",
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "test": "test",
    "build": "vite build && yarn tsc --project tsconfig.types.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "upgrade-storybook": "npx storybook@next upgrade --prerelease"
  },
  "dependencies": {
    "@buttery/icons": "workspace:^",
    "@buttery/utils": "workspace:^",
    "clsx": "^2.1.1",
    "dom": "^0.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@buttery/core": "workspace:^",
    "@buttery/docs": "workspace:^",
    "@chromatic-com/storybook": "^1.5.0",
    "@linaria/core": "^6.2.0",
    "@linaria/react": "^6.2.1",
    "@ngneat/falso": "^7.2.0",
    "@storybook/addon-essentials": "^8.1.10",
    "@storybook/addon-interactions": "^8.1.10",
    "@storybook/addon-links": "^8.1.10",
    "@storybook/blocks": "^8.1.10",
    "@storybook/react": "^8.1.10",
    "@storybook/react-vite": "^8.1.10",
    "@storybook/test": "^8.1.10",
    "@wyw-in-js/vite": "^0.5.3",
    "storybook": "^8.1.10",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  }
}
