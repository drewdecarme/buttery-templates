{
  "name": "buttery",
  "version": "1.0.1",
  "description": "A much needed CLI that helps manage setup boilerplate for common projects and utility sets for TS development projects",
  "main": "index.js",
  "private": true,
  "workspaces": {
    "packages": [
      "./packages/**",
      "./apps/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/drewdecarme/starter-ts-cli.git"
  },
  "author": "Drew DeCarme (drewdecarme@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drewdecarme/starter-ts-cli/issues"
  },
  "homepage": "https://github.com/drewdecarme/starter-ts-cli#readme",
  "packageManager": "yarn@4.3.0",
  "scripts": {
    "build": "turbo build",
    "release": "yarn build && changeset publish",
    "clean": "npx rimraf ./**/node_modules"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.1",
    "turbo": "^1.13.4"
  }
}
