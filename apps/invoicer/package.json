{
  "name": "invoicer",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "migrations:create": "npx wrangler d1 migrations create buttery-invoicer",
    "migrations:generate-sql": "tsx ./scripts/generate-sql.ts",
    "migrations:apply-local": "npx wrangler d1 migrations apply buttery-invoicer --local",
    "migrations:apply-remote": "npx wrangler d1 migrations apply buttery-invoicer --remote",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@buttery/components": "workspace:^",
    "@clerk/remix": "^4.0.1",
    "@icon-park/react": "^1.4.2",
    "@linaria/core": "^6.1.0",
    "@linaria/react": "^6.1.0",
    "@prisma/adapter-d1": "^5.13.0",
    "@prisma/client": "^5.13.0",
    "@remix-run/cloudflare": "^2.9.1",
    "@remix-run/cloudflare-pages": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "@wyw-in-js/babel-preset": "^0.5.1",
    "clsx": "^2.1.1",
    "immer": "^10.0.4",
    "install": "^0.13.0",
    "isbot": "^5.1.4",
    "miniflare": "^3.20240419.0",
    "npm": "^10.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remove": "^0.1.5",
    "ts-pattern": "^5.1.1",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@chromatic-com/storybook": "^1.3.3",
    "@cloudflare/workers-types": "^4.20240423.0",
    "@inquirer/prompts": "^5.0.1",
    "@remix-run/dev": "^2.9.1",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-onboarding": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-vite": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@wyw-in-js/vite": "^0.5.1",
    "eslint": "^9.1.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
